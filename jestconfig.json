{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testMatch": [
    "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs,ts,tsx}",
    "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs,ts,tsx}"
  ],
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "setupFilesAfterEnv": [
    "<rootDir>/scripts/setupTests.ts"
  ]
}